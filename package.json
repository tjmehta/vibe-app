{
  "name": "vibe-app",
  "version": "0.1.0",
  "private": true,
  "description": "Production-ready React Native + Expo starter with Convex, NativeWind, and EAS",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "dev": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:dev": "eas build --profile development --platform all",
    "build:preview": "eas build --profile preview --platform all",
    "build:prod": "eas build --profile production --platform all",
    "submit": "eas submit",
    "update": "eas update --auto",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "fix": "pnpm lint:fix && pnpm format",
    "test": "jest --watch",
    "test:run": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "maestro test .maestro/",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "@convex-dev/auth": "^0.0.90",
    "@convex-dev/react-query": "0.0.0-alpha.11",
    "@expo/vector-icons": "^14.0.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "@react-navigation/native": "^7.0.0",
    "@tanstack/react-query": "^5.90.2",
    "convex": "^1.27.3",
    "expo": "~54.0.31",
    "expo-auth-session": "~7.0.10",
    "expo-constants": "~18.0.13",
    "expo-crypto": "~15.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.10",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.21",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-updates": "~29.0.16",
    "expo-web-browser": "~15.0.10",
    "nativewind": "^4.1.23",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.0",
    "react-native-gesture-handler": "~2.30.0",
    "react-native-reanimated": "~4.2.1",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.19.0",
    "react-native-web": "~0.21.2",
    "tailwindcss": "^3.4.17",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@testing-library/react-native": "^12.8.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.3",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.8.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@9.0.0"
}
